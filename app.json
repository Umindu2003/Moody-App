{
  "expo": {
    "name": "Moody",
    "slug": "Moody",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Moody.png",
    "scheme": "moody",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/Moody.png",
        "backgroundImage": "./assets/images/Moody.png",
        "monochromeImage": "./assets/images/Moody.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Moody.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/MoodyLogo.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#f0fdf4"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/MoodyLogo.png",
          "color": "#4caf50"
        }
      ],
      "expo-print"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
